    <key>KeepAlive</key>
    <true/>
    <key>Label</key>
    <string>hashrocket.doorbell</string>
    <key>ProgramArguments</key>
    <array>
      <string>/usr/bin/env</string>
      <string>bundle</string>
      <string>exec</string>
